{
  "name": "aac-repair",
  "version": "1.1.1",
  "description": "Browser refuses to play aac file. Clean cut header and payload. Translation from Python project node.js.",
  "main": "./index.js",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "start": "node index.js",
    "build": "rollup -c && chmod a+x ./bin/aac-repair.js",
    "lint": "standard --verbose --fix",
    "posttest": "ls -1 *.md | xargs -t -n 1 markdown-link-check",
    "prepublishOnly": "npm run build",
    "pretest": "npm run lint && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/44xtc44/aacRepair.git"
  },
  "author": "44xtc44",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/44xtc44/aacRepair/issues"
  },
  "exports": {
    ".": "./index.js"
  },
  "keywords": [
    "aac",
    "aacp",
    "browser play aac repair"
  ],
  "homepage": "https://github.com/44xtc44/aacRepair/tree/dev/node.js/aac-repair-server#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.3",
    "express-fileupload": "^1.5.0",
    "fs-extra": "^11.2.0",
    "out-url": "^1.2.2"
  },
  "files": [
    "dist",
    "build",
    "LICENSE",
    "README.md",
    "aacWorker.js",
    "index.js",
    "server-aac.js",
    "aac-repair.js"
  ],
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-fileupload": "^1.4.4",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.11.28"
  },
  "bin": "bin/aac-repair.js"
}
